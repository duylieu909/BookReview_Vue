      <div class=\"form-element\">\n            <Field\n                name=\"name\"\n                type=\"name\"\n                class=\"form-control\"\n                v-model=\"authorLocal.name\"\n            />\n            </div>\n            <ErrorMessage name=\"name\" class=\"error-feedback\" /> \n        </div >\n        <div class=\"form-group\">\n            <label for=\"profile\" style=\"color: #5250C7\">Thông tin</label>\n            <div class=\"form-element\">\n            <Field\n                name=\"profile\"\n                as=\"textarea\"\n                class=\"form-control\"\n                v-model=\"authorLocal.profile\"\n            />\n            </div>\n            <ErrorMessage name=\"profile\" class=\"error-feedback\" />\n        </div>\n        <div class=\"form-group form-check\">\n            <input \n                name=\"favorite\"\n                type=\"checkbox\"\n                class=\"form-check-input\"\n                v-model=\"authorLocal.favorite\"\n            />\n            <label for=\"favorite\" class=\"form-check-label\" >\n                <strong style=\"color: #5250C7\">Yêu thích</strong>\n            </label>\n        </div>\n\n        <div class=\"form-group\">\n            <button class=\"btn btn-outline-secondary\">Lưu</button>\n            <button\n                v-if=\"authorLocal.id\"\n                type=\"button\"\n                class=\"ml-2 btn btn-outline-secondary\"\n                @click=\"$emit('author-delete', authorLocal.id)\"\n            >\n                Xóa\n            </button>\n        </div>\n\n    </form>\n</template>\n\n<script>\nimport * as yup from \"yup\";\nimport { Form, Field, ErrorMessage} from \"vee-validate\";\n\nexport default {\n    name: \"AuthorForm\",\n    components: {\n        Form,\n        Field,\n        ErrorMessage,\n    },\n    emits: [\"author-submit\", \"author-delete\"],\n    props: [\"author\"],\n    data() {\n        const schema = yup.object().shape