hành công.\");\n            }\n        },\n        async deleteReviewer() {\n            const [error, response] = await this.handle(\n                ReviewerService.delete(this.reviewer.id)\n            );\n            if (error) {\n                console.log(error);\n            } else {\n                console.log(response.data);\n                this.$router.push({ name: \"Reviewer\" });\n            }\n        },\n        },\n        mounted() {\n            this.message = \"\";\n            this.getReviewer(this.$route.params.id);\n        },\n    };\n</script>\n<style>\n.edit-form {\n    max-width: 400px;\n    margin: auto;\n}\n</style>"],"sourceRoot":""}]}