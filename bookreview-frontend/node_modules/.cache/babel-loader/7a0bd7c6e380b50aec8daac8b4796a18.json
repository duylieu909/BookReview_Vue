,QAKA;AAJL,QAAA,KAAK,EAAC,gCAID;AAHJ,QAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,iBAAE,IAAA,CAAA,KAAA,CAAK,eAAL,EAAuB,KAAA,CAAA,WAAA,CAAY,EAAnC,CAAF;AAAA,SAAA;AAGD,OAPT,EAKC,OALD,C,qCAQE,CAVN,CAzBO,CAXP;AAAA,K;;;;GAJJ,E;;AAAA,I,qBAAA,C","sourcesContent":["<template>\n    <Form \n        @submit=\"$emit('author-submit', authorLocal)\"\n        :validation-schema=\"schema\"    \n    >\n        <div class=\"form-group\">\n            <label for=\"name\" style=\"color: #5250C7\">Tên</label>\n            <div class=\"form-element\">\n            <Field\n                name=\"name\"\n                type=\"name\"\n                class=\"form-control\"\n                v-model=\"authorLocal.name\"\n            />\n            </div>\n            <ErrorMessage name=\"name\" class=\"error-feedback\" /> \n        </div >\n        <div class=\"form-group\">\n            <label for=\"profile\" style=\"color: #5250C7\">Thông tin</label>\n            <div class=\"form-element\">\n            <Field\n                name=\"profile\"\n                as=\"textarea\"\n                class=\"form-control\"\n                v-model=\"authorLocal.profile\"\n            />\n            </div>\n            <ErrorMessage name=\"profile\" class=\"error-feedback\" />\n        </div>\n        <div class=\"form-group form-check\">\n            <input \n                name=\"favorite\"\n                type=\"checkbox\"\n                class=\"form-check-input\"\n                v-model=\"authorLocal.favorite\"\n            />\n            <label for=\"favorite\" class=\"form-check-label\" >\n                <strong style=\"color: #5250C7\">Yêu thích</strong>\n            </label>\n        </div>\n\n        <div class=\"form-group\">\n            <button class=\"btn btn-outline-secondary\">Lưu</button>\n            <button\n                v-if=\"authorLocal.id\"\n                type=\"button\"\n                class=\"ml-2 btn btn-outline-secondary\"\n                @click=\"$emit('author-delete', authorLocal.id)\"\n            >\n                Xóa\n            </button>\n        </div>\n\n    </form>\n</template>\n\n<script>\nimport * as yup from \"yup\";\nimport { Form, Field, ErrorMessage} from \"vee-validate\";\n\nexport default {\n    name: \"AuthorForm\",\n    components: {\n        Form,\n        Field,\n        ErrorMessage,\n    },\n    emits: [\"author-submit\", \"author-delete\"],\n    props: [\"author\"],\n    data() {\n        const schema = yup.object().shape({\n            name: yup\n                .string()\n                .required(\"Tên phải có giá trị.\")\n                .min(2, \"Tên phải có ít nhất 2 ký tự.\")\n                .max(50, \"Tên có nhiều nhất 50 ký tự.\"),\n            profile: yup\n                .string(),           \n        });\n        return {\n            authorLocal: this.author,\n            schema,\n        };\n    },\n};\n</script>\n\n<style>\n    @import \"../assets/styles/main.css\";\n    \n</style>"],"sourceRoot":""}]}